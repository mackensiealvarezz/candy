{
    "version": 2,
    "public": true,
    "regions": ["all"],
    "builds": [{
        "src": "public/index.php",
        "use": "now-bref"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "/public/index.php"
    }],
    "build": {
        "env": {
            "NOW_PHP_DEBUG": "1"
        }
    }
}
